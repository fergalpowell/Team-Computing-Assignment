task main()
{	

  nMotorEncoderTarget[motorB] = 0;
	nMotorEncoderTarget[motorB] = 0;
	
	nMotorEncoderTarget[motorB] = 180;
	nMotorEncoderTarget[motorC] = 180;
	
	motor[motorB] = 50;
	motor[motorC] = 50;
	
	while(nMotorRunState[motorC] != runStateIdle)
	{
	}
	
	motor[motorB] = 0;
	motor[motorC] = 0;
	
}
